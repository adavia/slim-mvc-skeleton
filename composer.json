{
    "name": "adavia/slim-skeleton",
    "description": "Simple Slim Framework 4 skeleton with Twig, Monolog, Doctrine in Sqlite.",
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "src/Controllers",
            "App\\Models\\" : "src/Models",
            "App\\Middleware\\" : "src/Middleware",
            "App\\Renderer\\" : "src/Renderer"
        }
    },
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.0",
        "php-di/php-di": "^6.0",
        "slim/twig-view": "3.0.0-beta",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^4.1",
        "slim/flash": "^0.4.0",
        "illuminate/database": "^6.17",
        "robmorgan/phinx": "^0.11.4"
    }
}
